{
  "name": "arrayiterator/currency-rates",
  "description": "Currency Rate",
  "version": "1.0.0",
  "license": ["MIT"],
  "require": {
    "php": "^7.1",
    "ext-simplexml": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^6",
    "wa72/htmlpagedom": "^2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-0": {
      "ArrayIterator\\ProjectModule\\CurrencyRates\\": "src/"
    },
    "psr-4": {
      "ArrayIterator\\ProjectModule\\CurrencyRates\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "bin": [
    "bin/currency.php"
  ]
}